<%= simple_form_for(@question) do |f| %>
  <%= f.error_notification %>
<div class="col-md-6">
  <div class="form-inputs">
    <%= f.input :tile %>
    </div>
    <div clas="field">
    <%= f.input :body, as: :wysihtml5 %>
  </div>


  <div class="form-actions">
    <%= f.button :submit %>
  </div>
  </div>
<% end %>
